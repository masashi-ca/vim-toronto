execute pathogen#infect()
set laststatus=2
syntax on
filetype indent on
set tabstop=2
set shiftwidth=2
set showcmd
set hlsearch
set incsearch
set ruler
set nobackup
set nowrap
set foldmarker={,}
set foldmethod=marker
set foldlevelstart=0
set autoindent
set complete+=k
set number
set clipboard=unnamed
set matchtime=1
set incsearch
set modifiable
let g:NERDTreeWinSize=35
let python_highlight_all = 1
